import tkinter

window = tkinter.Tk()
window.title("my first gui program")
window.minsize(width=500,height=300)

#label

my_label = tkinter.Label(text="i am a label",font=("Arial",24,"bold"))
my_label.pack(side="left")
import turtle
tim =turtle.Turtle()
tim.write("some text",font=("Times New Roman",80))






window.mainloop()

